# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /testcases/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /testcases/index.php [L]
</IfModule>

# END WordPress


